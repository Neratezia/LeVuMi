$('#table').html("<%= escape_javascript(render file: 'measurements/_index', :formats => [:html])%>");

testWindow = window.open();
testWindow.location = '';
content = testWindow.document;

<% if @format == 'plain' %>
    content.write("<%= escape_javascript(render file: "results/tests/#{@test.view_info}_offline",  :layout => 'layouts/bare') %>");
<% else %>
    content.write("<%= escape_javascript(render file: "results/tests/#{@test.view_info}_online",  :layout => 'layouts/bare') %>");
<% end %>

content.close();